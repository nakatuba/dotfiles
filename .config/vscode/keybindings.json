[
    {
        "key": "ctrl+a",
        "command": "-vscode-neovim.ctrl-a-insert",
        "when": "editorTextFocus && neovim.ctrlKeysInsert && !neovim.recording && neovim.mode == 'insert'"
    },
    {
        "key": "ctrl+d",
        "command": "-editor.action.outdentLines",
        "when": "editorTextFocus && neovim.ctrlKeysInsert && !neovim.recording && neovim.mode == 'insert'"
    },
    {
        "key": "ctrl+f",
        "command": "editor.action.inlineSuggest.commit",
        "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorHoverFocused && !editorTabMovesFocus"
    },
    {
        "key": "ctrl+space",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.previousEditorInGroup",
        "when": "!inputFocus && !sideBarFocus || editorTextFocus && neovim.mode == 'normal'"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.nextEditorInGroup",
        "when": "!inputFocus && !sideBarFocus || editorTextFocus && neovim.mode == 'normal'"
    },
    {
        "key": "alt+p",
        "command": "moveActiveEditor",
        "args": {
            "to": "left"
        }
    },
    {
        "key": "alt+n",
        "command": "moveActiveEditor",
        "args": {
            "to": "right"
        }
    },
    {
        "key": "alt+h",
        "command": "workbench.action.moveActiveEditorGroupLeft",
        "when": "!inputFocus && !sideBarFocus || editorTextFocus && neovim.mode == 'normal'"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.moveActiveEditorGroupDown",
        "when": "!inputFocus && !sideBarFocus || editorTextFocus && neovim.mode == 'normal'"
    },
    {
        "key": "alt+k",
        "command": "workbench.action.moveActiveEditorGroupUp",
        "when": "!inputFocus && !sideBarFocus || editorTextFocus && neovim.mode == 'normal'"
    },
    {
        "key": "alt+l",
        "command": "workbench.action.moveActiveEditorGroupRight",
        "when": "!inputFocus && !sideBarFocus || editorTextFocus && neovim.mode == 'normal'"
    },
    {
        "key": "space f",
        "command": "workbench.action.quickOpen",
        "when": "!inputFocus"
    },
    {
        "key": "space g",
        "command": "workbench.action.findInFiles",
        "when": "!inputFocus"
    },
    {
        "key": "space b",
        "command": "workbench.action.showAllEditors",
        "when": "!inputFocus"
    },
    {
        "key": "space e",
        "command": "workbench.view.explorer",
        "when": "!inputFocus"
    },
    {
        "key": "s s",
        "command": "workbench.action.splitEditorDown",
        "when": "!inputFocus && !sideBarFocus"
    },
    {
        "key": "s v",
        "command": "workbench.action.splitEditorRight",
        "when": "!inputFocus && !sideBarFocus"
    },
    {
        "key": "s h",
        "command": "workbench.action.navigateLeft",
        "when": "!inputFocus"
    },
    {
        "key": "s j",
        "command": "workbench.action.navigateDown",
        "when": "!inputFocus"
    },
    {
        "key": "s k",
        "command": "workbench.action.navigateUp",
        "when": "!inputFocus"
    },
    {
        "key": "s l",
        "command": "workbench.action.navigateRight",
        "when": "!inputFocus"
    },
    {
        "key": "s q",
        "command": "workbench.action.closeEditorsInGroup",
        "when": "!inputFocus && !sideBarFocus"
    },
    {
        "key": "s o",
        "command": "workbench.action.closeEditorsInOtherGroups",
        "when": "!inputFocus && !sideBarFocus"
    },
    {
        "key": "s =",
        "command": "workbench.action.evenEditorWidths",
        "when": "!inputFocus && !sideBarFocus"
    },
    {
        "key": "ctrl+p",
        "command": "search.action.focusPreviousSearchResult",
        "when": "hasSearchResult || inSearchEditor"
    },
    {
        "key": "ctrl+n",
        "command": "search.action.focusNextSearchResult",
        "when": "hasSearchResult || inSearchEditor"
    }
]
